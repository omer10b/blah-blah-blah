function\n","def calc_tf(token, tokens_of_document):\n","  word_count = tokens_of_document.count(token)\n","  sentence_len = len(tokens_of_document)\n","  return word_count/sentence_len\n","\n","#IDF calculation function\n","def calc_idf(token, list_of_tokens_docs):\n","  word_count = 1\n","  for sentence in list_of_tokens_docs:\n","    if(sentence.count(token)>0):\n","      word_count+=1\n","  count_of_sentence = len(list_of_tokens_docs)\n","  return math.log(count_of_sentence / word_count)\n","\n","#TF-IDF calculation function\n","def calc_tf_idf(list_of_tokens_docs):\n","  tf_idf_dict = {}\n","  for sentence in list_of_tokens_docs:\n","    for word in sentence.split():\n","      tf_score =  calc_tf(word, sentence)\n","      idf_score = calc_idf(word,list_of_tokens_docs)\n","      tf_idf_dict[word] = tf_score*idf_score\n","\n","  return tf_idf_dict\n"]},{"cell_type":"code","execution_count":55,"metadata":{"id":"4P5QXSOrxoRo","executionInfo":{"status":"ok","timestamp":1703677408854,"user_tz":-120,"elapsed":249322,"user":{"displayName":"עומר ברסלב","userId":"05214264871792496505"}},"colab":{"base_uri":"https://localhost:8080/","height":0},"outputId":"901fb699-4286-4593-cae6-9bebe3a718dc"},"outputs":[{"output_type":"execute_result","data":{"text/plain":["        Words    tf_idf\n","0        unit  0.004046\n","1     continu  0.009180\n","2       engag  0.002523\n","3      intern  0.001897\n","4      climat  0.006217\n","...       ...       ...\n","8017   warili  0.003107\n","8018  quartet  0.003107\n","8019   dissip  0.003107\n","8020   cement  0.002905\n","8021   ruptur  0.003107\n","\n","[8022 rows x 2 columns]"],"text/html":["\n","  <div id=\"df-6af3e020-9eb5-4696-b2c5-2f6ca57e8066\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        tex