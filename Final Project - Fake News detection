   tokens = tokenizer.tokenize(text)\n","\n","    # Remove stop words and non-English words\n","    stop_words = set(stopwords.words('english'))\n","    english_words = set(words.words())\n","    tokens = [word for word in tokens if word not in stop_words and word in english_words]\n","\n","    # Remove stemming\n","    stemmer = PorterStemmer()\n","    tokens = [stemmer.stem(token) for token in tokens]\n","\n","    return tokens\n","'''"],"metadata":{"id":"JzuYGIQhby-e","executionInfo":{"status":"ok","timestamp":1703675976149,"user_tz":-120,"elapsed":32,"user":{"displayName":"עומר ברסלב","userId":"05214264871792496505"}},"colab":{"base_uri":"https://localhost:8080/","height":0},"outputId":"397eb34b-2ae4-4f3d-bf5b-bc4a895ef677"},"execution_count":37,"outputs":[{"output_type":"execute_result","data":{"text/plain":["\"\\nimport string\\nfrom nltk.corpus import stopwords\\nfrom nltk.tokenize import SpaceTokenizer\\nfrom nltk.stem import PorterStemmer\\nfrom nltk.corpus import words\\n\\ndef preprocess_text(text):\\n    # Lowercase the text\\n    text = text.lower()\\n\\n    # Remove punctuation using translation\\n    translator = str.maketrans('', '', string.punctuation + '’' + '“' + '”')\\n    text = text.translate(translator)\\n\\n    # Split the text into tokens\\n    tokenizer = SpaceTokenizer()\\n    tokens = tokenizer.tokenize(text)\\n\\n    # Remove stop words and non-English words\\n    stop_words = set(stopwords.words('english'))\\n    english_words = set(words.words())\\n    tokens = [word for word in tokens if word not in stop_words and word in english_words]\\n\\n    # Remove stemming\\n    stemmer = PorterStemmer()\\n    tokens = [stemmer.stem(token) for token in tokens]\\n\\n    return tokens\\n\""],"application/vnd.google.colaboratory.intrinsic+json":{"type":"string"}},"metadata":{},"execution_count":37}]},{"cell_type":"code","execution_count":38,"metadata":{"id":"sKADvuualzQp","executionInfo":{"status":"ok","timestamp":1703675976150,"user_tz":-120,"elapsed":32,"user":{"displayName":"עומר ברסלב","userId":"05214264871792496505"}}},"outputs":[],"source":["#Function called preprocess_text. It lower the text, remove punctuation, split the text into tokens, remove stemming\n","def preprocess_text(text):\n","  # Lowercase the text\n","  text = text.lower()\n","  # Remove punctuation\n","  punctuations = string.punctuation + '’' + '“' + '”'\n","  text = ''.join(char for char in text if char not in punctuations)\n","  # Split the text into tokens\n","  tokenizer = SpaceTokenizer()\n","  tokens = tokenizer.tokenize(text)\n","  # Remove stop words\n","  stop_words = set(stopwords.words('english'))\n","  tokens = [word for word in tokens if word not in stop_words]\n","  #Words dict\n","  english_words = set(words.words())\n","  tokens =  [word for word in tokens if word in english_words]\n","  # Remove stemming\n","  stemmer = PorterStemmer()\n","  tokens = [stemmer.stem(token) for token in tokens]\n","  return tokens"]},{"cell_type":"code","execution_count":39,"metadata":{"id":"MJqbu_APmTYv","executionInfo":{"status":"ok","timestamp":1703677133559,"user_tz":-120,"elapsed":1157440,"user":{"displayName":"עומר ברסלב","userId":"05214264871792496505"}},"colab":{"base_uri":"https://localhost:8080/"},"outputId":"9783434b-11ce-4145-cabe-64964bfaab60"},"outputs":[{"output_type":"execute_result","data":{"text/plain":["0        [unit, continu, engag, intern, climat, chang, ...\n","1        [presid, trump, new, polici, us, militari, mad...\n","2        [us, republican, presidenti, candid, trump, sa...\n","3        [kremlin, said, confid, unit, attempt, turn, r...\n","4        [respons, critic, come, expect, classi, absolu...\n","                               ...                        \n","10224    [deputi, prime, minist, resign, els, forc, sna...\n","10225    [power, letter, lifer, lifer, militari, seen, ...\n","10226    [prime, minist, said, hold, referendum, indepe...\n","10227    [presid, macron, world, arrogantli, taken, cou...\n