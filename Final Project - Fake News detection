tities\n","def NER(text):\n","    doc = nlp(text)\n","    entities = [(ent.text, ent.label_) for ent in doc.ents]\n","    return entities\n","\n","#Create NER to the first 10000 rows.\n","entities = news['text'][:10000].apply(NER);"]},{"cell_type":"code","source":["entitels_dict = {}\n","for entitle in entities:\n","  for e in entitle:\n","    if(e in entitels_dict.keys()):\n","      entitels_dict[e] +=1\n","    else:\n","      entitels_dict[e] = 1\n","\n","sorted_entitels = sorted(entitels_dict.items(), key=lambda x: x[1], reverse=True)\n","entitles_df= pd.DataFrame(sorted_entitels, columns=['Entitles', 'Count'])\n","entitles_df"],"metadata":{"id":"XzJQTe0oV0rV","colab":{"base_uri":"https://localhost:8080/","height":0},"executionInfo":{"status":"ok","timestamp":1703678168045,"user_tz":-120,"elapsed":32,"user":{"displayName":"עומר ברסלב","userId":"05214264871792496505"}},"outputId":"20bd36b2-e8f2-41b2-e0a5-dba65978d259"},"execution_count":61,"outputs":[{"output_type":"execute_result","data":{"text/plain":["                                      Entitles  Count\n","0                                 (Trump, ORG)  19595\n","1                                  (U.S., GPE)   9250\n","2                           (Republican, NORP)   5169\n","3                       (Donald Trump, PERSON)   5120\n","4                     (the United States, GPE)   3819\n","...                                        ...    ...\n","71243  (Royal Bank of Scotland Group PLC, ORG)      1\n","71244                             (RBS.L, ORG)      1\n","71245                            (UBSG.S, GPE)      1\n","71246                 (Eric Schaeffer, PERSON)      1\n","71247               (Harley-Davidson Inc, ORG)      1\n","\n","[71248 rows x 2 columns]"],"text/html":["\n","  <div id=\"df-e0ef41ea-4ffa-4ec2-906f-df73308fef8f\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .datafram